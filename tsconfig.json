{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./server/src",
    "outDir": "./server/dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["server/src/**/*"],
	"exclude": ["node_modules", "**/*.spec.ts", "client"]
}
